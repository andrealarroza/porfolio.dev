---
import ExperienceItem from "./ExperienceItem.astro"
const EXPERIENCIE = [
  {
    date: "Actualmente",
    title: "Diseñadora web",
    company: "Agencia Porta",
    description:
      "Desarrollé interfaces de usuario para sitios institucionales e ecommerce a medida con HTML, CSS y JavaScript. Optimicé sitios web para ofrecer la máxima velocidad y escalabilidad, garantizando la compatibilidad con distintos navegadores y dispositivos.",
    // link: "https://porta.com.py/",
  },
]
---

<ol class="relative mt-16">
  {
    EXPERIENCIE.map((experiencie) => (
      <li class="">
        <ExperienceItem {...experiencie} />
      </li>
    ))
  }
</ol>
